	<software name="vw64">
		<description>VizaWrite 64</description>
		<year>1984</year>
		<publisher>Viza Software</publisher>
		<!--
		|===========================|
		|=|      LS10    LS122  C   |
		|=|                  R      |
		|=|                         |
		|=|                         |
		|=|      ROM0  ROM2  ROM1   |
		|=|                         |
		|=|                         |
		|=|                         |
		|===========================|

		ROM0    - Mitsubishi M5L2764K 8Kx8 EPROM "U"
		ROM1    - Mitsubishi M5L2764K 8Kx8 EPROM "3"
		ROM2    - Mitsubishi M5L2764K 8Kx8 EPROM "2" (located on solder side)
		R       - 56K
		C       - 47uF

		data and address lines are scrambled:
		data = BITSWAP8(7,6,0,5,1,4,2,3)
		address = BITSWAP16(15,14,13,12,7,8,6,9,5,11,4,3,2,10,1,0)
		GAME:after reset GND for 1.1844s (74LS122 tW=0.45*R*C) then goes to Vcc
		EXROM:GND
		-->
		<part name="cart" interface="c64_cart">
			<feature name="cart_type" value="vizawrite" />
			<feature name="game" value="0" />
			<feature name="exrom" value="0" />

			<dataarea name="roml" size="0x4000">
				<rom name="u" size="0x2000" crc="b0cc7564" sha1="59f7b3a9df26053fdcb720fe07a22f437a7a2241" offset="0x0000" />
				<rom name="3" size="0x2000" crc="ef80b1e7" sha1="80392a04bc9ca2fea006653a507f6f5338a10beb" offset="0x2000" />
			</dataarea>

			<dataarea name="romh" size="0x2000">
				<rom name="2" size="0x2000" crc="32f2a5df" sha1="5f3e5d416eab59a75ba64131588861991fa571a5" offset="0x0000" />
			</dataarea>
		</part>
	</software>

