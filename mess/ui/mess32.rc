                    WS_TABSTOP
	LTEXT			"Printer",IDC_PRINTER_CAPTION,20,70,50,10,SS_NOPREFIX
	EDITTEXT		IDC_PRINTER_EDITTEXT,70,70,85,14, ES_AUTOHSCROLL | ES_LEFT,0
	PUSHBUTTON		"Browse",IDC_PRINTER_BROWSE,158,70,40,14,0,0
END

